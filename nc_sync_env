export NC_PATH="/usr/bin"
export NC_PROCES="nextcloudcmd"
export NC_LINUX_UNAME="[USERNAME]"
export NC_UNAME="[NEXTCLOUD_USERNAME]"
export NC_PWORD="[NEXTCLOUD_PASSWORD]"
export NC_EXCLUDFILE="$NC_USERCMD_DIR/excluded"
export NC_LOCALDIR="/home/$NC_LINUX_UNAME/nextcloud"
export NC_SERVER="https://nextcloud.example.com"
export NC_CMD="$NC_PATH/$NC_PROCES -s -u $NC_UNAME -p $NC_PWORD --unsyncedfolders $NC_EXCLUDFILE --non-interactive $NC_LOCALDIR $NC_SERVER"
export NC_LOGFILE="/var/log/nextcloudsync.log"
export NC_USER_CMD="/usr/bin/bash $NC_USERCMD_DIR/nextcloud_cmd.sh"
export NC_SLEEPTIME=151
